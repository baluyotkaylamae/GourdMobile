{
  "expo": {
    "name": "GourdMobile",
    "slug": "GourdMobile",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoNBG.png",
      "resizeMode": "center",
      "backgroundColor": "#fff"
    },
    "permissions": [
      "CAMERA",
      "CAMERA_ROLL"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoNBG.png",
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "gourdmobile"  
  }}
